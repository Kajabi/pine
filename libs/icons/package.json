{
  "name": "@sage/icons",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Sage Icon Library",
  "author": " Kajabi Design System Services",
  "keywords": [
    "icon",
    "icon library",
    "sage",
    "svg",
    "web component"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sage-icons/icons.esm.js",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kajabi/sage/libs/icons"
  },
  "bugs": {
    "url": "https://github.com/kajab/sage/issues"
  },
  "scripts": {
    "build": "run-s build.ts build.icons build.component collection.copy",
    "build.dev": "run-s build.ts build.icons build.component.dev collection.copy",
    "build.component": "stencil build --docs",
    "build.component.dev": "stencil build --dev --docs --watch --serve",
    "build.icons": "run-func scripts/optimizeSvg.js build $(pwd)",
    "build.ts": "tsc -p scripts/tsconfig.json",
    "collection.copy": "run-func scripts/collection-copy.js collectionCopy $(pwd)",
    "figma.export": "node scripts/figma_icons/figma-icon-export.js",
    "lint": "run-p lint.*",
    "lint.eslint": "eslint src/**/*.{ts,tsx}",
    "start": "run-s build.dev",
    "test": "stencil test --spec --e2e",
    "test.spec": "stencil test --spec",
    "test.spec.watch": "stencil test --spec --watch",
    "update": "run-s build.ts figma.export"
  },
  "files": [
    "components/",
    "dist/",
    "icons/"
  ],
  "dependencies": {
    "@stencil/core": "^2.18.0"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/sass": "^2.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/mkdirp": "^1.0.2",
    "@types/svgo": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "axios": "^1.1.3",
    "chalk": "^4.1.2",
    "cliui": "^8.0.1",
    "eslint": "^8.27.0",
    "fs-extra": "^10.1.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "puppeteer": "^10.0.0",
    "run-func": "^3.0.0",
    "simple-git": "^3.16.0",
    "svgo": "^3.0.0",
    "typescript": "^4.8.4"
  }
}
