{
  "defaultBase": "origin/main",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/loader",
        "{projectRoot}/www"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
