{
  "affected": {
    "defaultBase": "origin/main"
  },
  "npmScope": "sage",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "e2e",
          "lint",
          "test"
        ],
        "accessToken": "OTljYTlkYjEtYzk5Ni00ZWZkLTkyYTItNGMzZTM2NDRmZjdmfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/loader",
        "{projectRoot}/www"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
